\begin_layout Subsection
dbtables
\end_layout

\begin_layout Standard
Module contains class definitions of all MS data types: IDrun, Compound,
 Adduct, Isotope.
\end_layout

\begin_layout Description
Required
\begin_inset space \space{}
\end_inset

Modules
\end_layout

\begin_deeper
\begin_layout Labeling
\labelwidthstring 05.50
sqlalchemy
\begin_inset Argument
status open

\begin_layout Plain Layout
labelindent=0.5em
\end_layout

\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 05.50
btools
\end_layout

\end_deeper
\begin_layout Description
