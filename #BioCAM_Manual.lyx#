\begin_layout Subsection
dynamic_gexf
\end_layout

\begin_layout Standard
command line program for importing cef files into a MS database.
\end_layout

\begin_layout Description
Command
\begin_inset space \space{}
\end_inset

Line
\begin_inset space \space{}
\end_inset

Usage
\end_layout

\begin_layout Standard
\begin_inset listings
lstparams "basicstyle={\small\ttfamily},frame=single,keywordstyle={\color{blue}},language=bash"
inline false
status open

\begin_layout Plain Layout

usage: cefimport.py [-h] [--files CEFS [CEFS ...]]
\end_layout

\begin_layout Plain Layout

[--blanks BLANKS [BLANKS ...]] [--db DB] [--cpus CPUS]
\end_layout

\begin_layout Plain Layout

[--cutoff CUTOFF] [--minrt RT]
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

optional arguments:
\end_layout

\begin_layout Plain Layout

-h, - -help
\end_layout

\begin_layout Plain Layout

	show this help message and exit
\end_layout

\begin_layout Plain Layout

--files CEFS [CEFS ...], -f CEFS [CEFS ...]
\end_layout

\begin_layout Plain Layout

	The .cef files to import into the database.
\end_layout

\begin_layout Plain Layout

	There must be positive/negative mode and 4GHz/2GHz mode
\end_layout

\begin_layout Plain Layout

	data for each idrun, and the location code, extract,
\end_layout

\begin_layout Plain Layout

	fraction, and minute motif must be the same for all
\end_layout

\begin_layout Plain Layout

	datasets for one idrun.
\end_layout

\begin_layout Plain Layout

--blanks BLANKS [BLANKS ...], -b BLANKS [BLANKS ...]
\end_layout

\begin_layout Plain Layout

	The .cef files to subtract as blanks from the other cef
\end_layout

\begin_layout Plain Layout

	files.
 The same mode restrictions apply.
\end_layout

\begin_layout Plain Layout

--db DB, -d DB
\end_layout

\begin_layout Plain Layout

	The database to write the mass spectral data
\end_layout

\begin_layout Plain Layout

--cpus CPUS, -n CPUS
\end_layout

\begin_layout Plain Layout

	The number of CPU cores to use parallely when processing
\end_layout

\begin_layout Plain Layout

	data.
 Default is all that are available.
\end_layout

\begin_layout Plain Layout

--cutoff CUTOFF, -c CUTOFF
\end_layout

\begin_layout Plain Layout

	Default minimum abundance cutoff for compounds, default
\end_layout

\begin_layout Plain Layout

	is 1000.
\end_layout

\begin_layout Plain Layout

--minrt RT, -rt RT
\end_layout

\begin_layout Plain Layout

	Minimum retention time for compounds to avoid solvent
\end_layout

\begin_layout Plain Layout

	front.
 Default is 0.35
\end_layout

\end_inset


\end_layout

